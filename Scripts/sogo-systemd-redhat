[Unit]
Description=SOGo is a groupware server
After=network.target memcached.service postfix.service dovecot.service postgresql.service

[Service]
Environment="PREFORK=3"
EnvironmentFile=-/etc/sysconfig/sogo
ExecStart=/usr/sbin/sogod -WOWorkersCount ${PREFORK} -WOPidFile /var/run/sogo/sogo.pid -WONoDetach YES -WOLogFile /var/log/sogo/sogo.log
PIDFile=/var/run/sogo/sogo.pid
User=sogo

[Install]
WantedBy=multi-user.target
